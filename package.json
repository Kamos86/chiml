{
  "name": "neo-chimera",
  "version": "0.0.0",
  "description": "Better version of Chimera-Framework",
  "main": "dist/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint && tsc --sourcemap",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goFrendiAsgard/chimera-framework.git"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/.*\\.test\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "verbose": true,
    "collectCoverage": true
  },
  "keywords": [
    "Orchestration"
  ],
  "author": "goFrendi",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goFrendiAsgard/chimera-framework/issues"
  },
  "homepage": "https://github.com/goFrendiAsgard/chimera-framework#readme",
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/js-yaml": "^3.11.1",
    "@types/node": "^10.3.2",
    "@types/request": "^2.47.0",
    "express": "^4.16.3",
    "js-yaml": "^3.12.0",
    "request": "^2.87.0",
    "typescript": "^2.9.1"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "eslint-config-google": "^0.9.1",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6"
  }
}
